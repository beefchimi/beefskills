{
  "name": "beefskills",
  "version": "1.1.0",
  "private": true,
  "description": "A collection of Claude skills",
  "keywords": [
    "Claude",
    "skills"
  ],
  "homepage": "https://github.com/beefchimi/beefskills#readme",
  "bugs": {
    "url": "https://github.com/beefchimi/beefskills/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beefchimi/beefskills.git"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules/.cache && rm -rf node_modules/.vite",
    "nuke": "npm run clean && rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force",
    "build:skills": "find plugins -name 'build_agents.py' -exec python3 {} \\;",
    "audit:quotes": "python3 scripts/audit_fancy_quotes.py",
    "bump:version": "python3 scripts/bump_plugin_version.py",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "lint:force": "oxlint --fix-dangerously",
    "format": "oxfmt --check",
    "format:fix": "oxfmt"
  },
  "devDependencies": {
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "typescript": "^5.9.3"
  }
}
